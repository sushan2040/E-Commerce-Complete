{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "ecommerce",
    "containerDefinitions": [
        {
            "name": "ecommerce",
            "image": "235494784067.dkr.ecr.eu-north-1.amazonaws.com/ecommerce:latest",
			"portMappings": [
			                {
			                    "name": "tomcat-8080-http",
			                    "containerPort": 8080,
			                    "hostPort": 8080,
			                    "protocol": "tcp",
			                    "appProtocol": "http"
			                }
			            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::235494784067:role/ecsTaskExecutionRole"
}